---
layout: "layout.njk"
title: Services
page_title: Services Provide by FTPTA
tag: "services"
---
<header class="content_header">
    <h2>{{ page_title }}</h2>
</header>
<div class="services">
{% for service in services %}
    <section class="card">
        <a href="{{ service.url }}">
            <div class="card-header">
                <h3>{{ service.header }}</h3>
            </div>
            <article class="card-content">
                <p>{{ service.details }}</p>
                <h4>{{ service.price }}</h4>
            </article>
        </a>
    </section>
{% endfor %}
</div>
