---
layout: "layout.njk"
title: Services
page_title: Services Provide by FTPTA
tag: "services"
---
<header class="content_header">
    <h2>{{ page_title }}</h2>
</header>
<div class="services">
{% for service in services %}
    <section class="card">        
        <div class="card-header">
            <a href="{{ service.url }}"><h3>{{ service.header }}</h3></a>
        </div>
        <article class="card-content">
            <p>{{ service.details }}</p>
            <h4>{{ service.price }}</h4>
        </article>        
    </section>
{% endfor %}
</div>
